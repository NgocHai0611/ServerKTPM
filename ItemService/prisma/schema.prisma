generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model products {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  idProduct String
  productName String
  unitPrice Float
  imgProduct String
  desc     String
  size String
}



